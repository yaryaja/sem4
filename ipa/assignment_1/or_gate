#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55843815f120 .scope module, "or_test" "or_test" 2 1;
 .timescale 0 0;
v0x558438170740_0 .var "a", 0 0;
v0x558438170810_0 .var "b", 0 0;
v0x5584381708e0_0 .net "c", 0 0, v0x558438170620_0;  1 drivers
S_0x55843815f2a0 .scope module, "or_test" "or_gate" 2 4, 3 1 0, S_0x55843815f120;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a"
    .port_info 1 /INPUT 1 "b"
    .port_info 2 /OUTPUT 1 "c"
v0x558438129030_0 .net "a", 0 0, v0x558438170740_0;  1 drivers
v0x558438170560_0 .net "b", 0 0, v0x558438170810_0;  1 drivers
v0x558438170620_0 .var "c", 0 0;
E_0x55843815f850 .event edge, v0x558438170560_0, v0x558438129030_0;
    .scope S_0x55843815f2a0;
T_0 ;
    %wait E_0x55843815f850;
    %load/vec4 v0x558438129030_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x558438170560_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558438170620_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558438170620_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55843815f120;
T_1 ;
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558438170740_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558438170810_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558438170740_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558438170810_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558438170740_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558438170810_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558438170740_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558438170810_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55843815f120;
T_2 ;
    %vpi_call 2 14 "$monitor", $time, "a=%b,b=%b,c=%b", v0x558438170740_0, v0x558438170810_0, v0x5584381708e0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
