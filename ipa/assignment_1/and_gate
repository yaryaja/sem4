#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5611a3153120 .scope module, "and_test" "and_test" 2 1;
 .timescale 0 0;
v0x5611a3164740_0 .var "a", 0 0;
v0x5611a3164810_0 .var "b", 0 0;
v0x5611a31648e0_0 .net "c", 0 0, v0x5611a3164620_0;  1 drivers
S_0x5611a31532a0 .scope module, "and_test" "and_gate" 2 4, 3 1 0, S_0x5611a3153120;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "c"
    .port_info 1 /INPUT 1 "a"
    .port_info 2 /INPUT 1 "b"
v0x5611a311d030_0 .net "a", 0 0, v0x5611a3164740_0;  1 drivers
v0x5611a3164560_0 .net "b", 0 0, v0x5611a3164810_0;  1 drivers
v0x5611a3164620_0 .var "c", 0 0;
E_0x5611a3153850 .event edge, v0x5611a3164560_0, v0x5611a311d030_0;
    .scope S_0x5611a31532a0;
T_0 ;
    %wait E_0x5611a3153850;
    %load/vec4 v0x5611a311d030_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5611a3164560_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5611a3164620_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5611a3164620_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5611a3153120;
T_1 ;
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5611a3164740_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5611a3164810_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5611a3164740_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5611a3164810_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5611a3164740_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5611a3164810_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5611a3164740_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5611a3164810_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5611a3153120;
T_2 ;
    %vpi_call 2 14 "$monitor", $time, "a=%b,b=%b,c=%b", v0x5611a3164740_0, v0x5611a3164810_0, v0x5611a31648e0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "and_tb.v";
